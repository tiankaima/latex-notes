@article{RN1,
  author   = {胡耀娟 and 金娟 and 张卉 and 吴萍 and 蔡称心},
  title    = {石墨烯的制备、功能化及在化学中的应用},
  journal  = {物理化学学报},
  volume   = {26},
  number   = {08},
  pages    = {2073-2086},
  abstract = {石墨烯是最近发现的一种具有二维平面结构的碳纳米材料,它的特殊单原子层结构使其具有许多独特的物理化学性质.有关石墨烯的基础和应用研究已成为当前的前沿和热点课题之一.本文仅就目前石墨烯的制备方法、功能化方法以及在化学领域中的应用作一综述,重点阐述石墨烯应用于化学修饰电极、化学电源、催化剂和药物载体以及气体传感器等方面的研究进展,并对石墨烯在相关领域的应用前景作了展望.},
  keywords = {石墨烯
              碳材料
              石墨烯氧化物
              石墨烯功能化
              石墨烯应用},
  issn     = {1000-6818},
  year     = {2010},
  type     = {Journal Article}
}

@article{RN10,
